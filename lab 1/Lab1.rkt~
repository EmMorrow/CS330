#lang racket
(define (sum-coins pennies nickels dimes quarters)
  (+ pennies
     (* 5 nickels)
     (* 10 dimes)
     (* 25 quarters)))

(sum-coins 5 1 1 1)

(define (degrees-to-radians angle)
  (* angle
     (/  3.14159 180)))
(degrees-to-radians 15)


(define (sin x)
  (if (> x 0)
      1
      (if (equal? x 0)
          0
          -1)))
 
(sin 5)

